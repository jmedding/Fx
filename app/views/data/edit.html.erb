<h1>Editing datum</h1>

<% form_for(@datum) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :conversion_id %><br />
    <%= f.text_field :conversion_id %>
  </p>
  <p>
    <%= f.label :rate %><br />
    <%= f.text_field :rate %>
  </p>
  <p>
    <%= f.label :day %><br />
    <%= f.date_select :day %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @datum %> |
<%= link_to 'Back', data_path %>