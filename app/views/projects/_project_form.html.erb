<% form_for(project) do |f| %>
  <%= f.error_messages %>
  
  <p>
	 <%= "Created by #{project.user.name}" %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows => 3, :cols => 40 %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>